    },
    "deskripsi": {
      "type": "blocks"
    },
    "tanggal_mulai": {
      "type": "datetime"
    },
    "tanggal_selesai": {
      "type": "datetime"
    },
    "lokasi": {
      "type": "string"
    },
    "tag_kegiatan": {
      "type": "enumeration",
      "enum": [
        "sholat",
        "pengajian",
        "event",
        "ramadhan"
      ]
    },
    "gambar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
